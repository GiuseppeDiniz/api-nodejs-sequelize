{
  "name": "api-nodejs-sequelize",
  "version": "1.0.0",
  "description": "The package.json API for Node.js with Sequelize is a powerful combination for managing dependencies and configuring a Node.js project.",
  "main": "api/server.js",
  "repository": "https://github.com/GiuseppeDiniz/api-nodejs-sequelize",
  "author": "Giuseppe Diniz",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node-dev ./api/server.ts\"",
    "build": "tsc",
    "start": "yarn run build && node dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
